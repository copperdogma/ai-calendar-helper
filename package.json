{
  "name": "{{YOUR_PROJECT_NAME}}",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev | pino-pretty",
    "dev:3001": "cross-env PORT=3001 next dev -p 3001 | pino-pretty",
    "dev:test": "cross-env NODE_ENV=test ALLOW_TEST_ENDPOINTS=true NEXT_PUBLIC_IS_E2E_TEST_ENV=true next dev --port 3777 | tee test-server.log",
    "build": "next build",
    "start": "next start",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint .",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint --fix .",
    "lint:files": "ESLINT_USE_FLAT_CONFIG=true eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:all": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json,css,html}\"",
    "dev:clean": "npm run format && npm run lint:fix && npm run dev",
    "build:clean": "npm run format && npm run lint:fix && npm run build",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build:analyze": "ANALYZE=true next build",
    "validate": "npm run lint && npm run format:check && npm run type-check",
    "prepare": "husky",
    "test": "npm run test:unit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:unit": "cross-env NODE_ENV=test jest --passWithNoTests --coverage",
    "test:unit:watch": "cross-env NODE_ENV=test jest --watch",
    "test:unit:subset": "cross-env NODE_ENV=test jest --findRelatedTests",
    "test:unit:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:unit:coverage:subset": "cross-env NODE_ENV=test JEST_SUBSET_COVERAGE=true jest --coverage --findRelatedTests",
    "test:e2e": "npx kill-port 8080 9099 4000 4400 4500 3777 &> /dev/null || true && rm -f test-server.log && firebase emulators:exec --only auth --import=./firebase-seed-data --project next-firebase-base-template \"start-server-and-test \\\"rm -rf .next && npm run dev:test\\\" http://localhost:3777 \\\"npx playwright test\\\"\" && node scripts/check-server-logs.js test-server.log",
    "test:e2e:auth-only": "npx kill-port 8080 9099 4000 4400 4500 3777 &> /dev/null || true && rm -f test-server.log && firebase emulators:exec --only auth --import=./firebase-seed-data --project next-firebase-base-template \"start-server-and-test \\\"rm -rf .next && npm run dev:test\\\" http://localhost:3777 \\\"npx playwright test tests/e2e/auth/ tests/e2e/public-access.spec.ts tests/e2e/profile/edit-profile.spec.ts\\\"\" && node scripts/check-server-logs.js test-server.log",
    "test:e2e:help": "npx playwright test --help",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:ci": "cross-env CI=true npm run test:e2e",
    "test:ci": "npm run test:unit && npm run test:e2e",
    "test:e2e:against-existing": "cross-env USE_EXISTING_SERVER=true PLAYWRIGHT_TEST_BASE_URL=http://localhost:3777 playwright test",
    "test:e2e:custom-url": "cross-env PLAYWRIGHT_TEST_BASE_URL=http://localhost:3000 playwright test",
    "firebase:emulators": "firebase emulators:start --only auth --project next-firebase-base-template",
    "firebase:emulators:export": "firebase emulators:export ./firebase-seed-data --project next-firebase-base-template",
    "firebase:emulators:import": "firebase emulators:start --project next-firebase-base-template --import=./firebase-seed-data --export-on-exit --debug",
    "firebase:setup-test-user": "node scripts/setup-test-user.js",
    "firebase:clear-data": "node scripts/clear-emulator-data.js",
    "firebase:emulators:demo": "firebase emulators:start --only auth --project demo-test",
    "firebase:update-seed-data": "node scripts/update-seed-data.js",
    "emulator:setup-user": "node scripts/setup-test-user.js",
    "ai:start": "pm2 start ecosystem.config.js --env development && sleep 5 && npm run ai:port && npm run ai:verify-port",
    "ai:stop": "pm2 stop next-dev",
    "ai:restart": "pm2 restart next-dev --update-env && rm -f .next-port && sleep 5 && npm run ai:port && npm run ai:verify-port && npm run ai:health",
    "ai:verify-port": "node scripts/verify-port.js",
    "ai:status": "pm2 status next-dev",
    "ai:logs": "pm2 logs next-dev --lines 100 --nostream",
    "ai:port": "node scripts/get-port.js",
    "ai:health": "node scripts/check-health.js",
    "dev:simple": "next dev -p 3336"
  },
