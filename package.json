{
  "name": "{{YOUR_PROJECT_NAME}}",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build:analyze": "ANALYZE=true next build",
    "validate": "npm run lint && npm run format:check && npm run type-check",
    "prepare": "husky",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch --coverage",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:ci": "cross-env NODE_ENV=test jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test --config tests/config/playwright.config.ts",
    "test:e2e:ui": "playwright test --config tests/config/playwright.config.ts --ui",
    "test:e2e:debug": "playwright test --config tests/config/playwright.config.ts --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:dynamic": "ts-node tests/config/run-tests.ts",
    "test:e2e:nav": "PLAYWRIGHT_TEST_BASE_URL=http://localhost:3001 playwright test tests/e2e/navigation.spec.ts",
    "test:e2e:navigation": "./tests/run-navigation-tests.sh",
    "test:e2e:navigation:chromium": "./tests/run-navigation-tests.sh --project=chromium",
    "test:e2e:simple": "./tests/simple-test-runner.sh",
    "test:e2e:direct": "PLAYWRIGHT_TEST_BASE_URL=http://localhost:3001 playwright test tests/e2e/debug.spec.ts --project=chromium",
    "test:debug-basic": "npx playwright test tests/e2e/debug.spec.ts",
    "setup": "node scripts/setup.js"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@mui/x-date-pickers": "^7.28.0",
    "@prisma/client": "^6.5.0",
    "@types/firebase": "^3.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^11.4.0",
    "firebase-admin": "^13.2.0",
    "next": "15.2.2",
    "next-pwa": "^5.6.0",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "webpack": "^5.98.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@babel/plugin-transform-typescript": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.2.2",
    "@playwright/test": "^1.51.1",
    "@tailwindcss/postcss": "^4.0.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "inquirer": "^9.3.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.7.3",
    "node-mocks-http": "^1.16.2",
    "postcss": "^8.5.3",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "tailwindcss": "^4.0.14",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "description": "{{YOUR_PROJECT_DESCRIPTION}}",
  "repository": {
    "type": "git",
    "url": "{{YOUR_REPOSITORY_URL}}"
  }
}
