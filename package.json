{
  "name": "{{YOUR_PROJECT_NAME}}",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "prettier --write .",
    "dev": "next dev",
    "prebuild": "prettier --write .",
    "build": "next build",
    "start": "next start",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint .",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint --fix .",
    "lint:files": "ESLINT_USE_FLAT_CONFIG=true eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:all": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json,css,html}\"",
    "dev:clean": "npm run format && npm run lint:fix && npm run dev",
    "build:clean": "npm run format && npm run lint:fix && npm run build",
    "type-check": "tsc",
    "clean": "rm -rf .next out",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build:analyze": "ANALYZE=true next build",
    "validate": "npm run lint && npm run format:check && npm run type-check",
    "prepare": "husky",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:unit": "cross-env NODE_ENV=test jest tests/unit",
    "test:integration": "cross-env NODE_ENV=test jest tests/integration",
    "test:e2e": "playwright test --config tests/config/playwright.config.ts",
    "test:e2e:ui": "playwright test --ui --config tests/config/playwright.config.ts",
    "test:e2e:debug": "playwright test --config tests/config/playwright.config.ts --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:dynamic": "ts-node --transpile-only tests/config/run-tests.ts",
    "test:e2e:dynamic:optimized": "NODE_OPTIONS='--max-old-space-size=4096' ts-node --transpile-only tests/config/run-tests.ts",
    "test:e2e:dynamic:paths": "ts-node --transpile-only -r tsconfig-paths/register tests/config/run-tests.ts",
    "test:e2e:dynamic:tsx": "tsx tests/config/run-tests.ts",
    "test:e2e:nav": "PLAYWRIGHT_TEST_BASE_URL=http://localhost:3001 playwright test tests/e2e/navigation.spec.ts",
    "test:e2e:navigation": "./tests/run-navigation-tests.sh",
    "test:e2e:navigation:chromium": "./tests/run-navigation-tests.sh --project=chromium",
    "test:e2e:simple": "./tests/simple-test-runner.sh",
    "test:e2e:direct": "PLAYWRIGHT_TEST_BASE_URL=http://localhost:3001 playwright test tests/e2e/debug.spec.ts --project=chromium",
    "test:debug-basic": "npx playwright test tests/e2e/debug.spec.ts",
    "test:e2e:sequential": "playwright test --config tests/config/playwright.config.ts --workers=1",
    "test:e2e:reliable": "npm run dev -- --port 3001 & sleep 10 && PLAYWRIGHT_TEST_BASE_URL=http://127.0.0.1:3001 playwright test --config tests/config/playwright.config.ts --workers=1",
    "firebase:emulators": "firebase emulators:start --only firestore",
    "firebase:emulators:export": "firebase emulators:export ./firebase-emulator-data",
    "firebase:emulators:import": "firebase emulators:start --import=./firebase-emulator-data",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules",
    "test:rules": "jest tests/firebase/security-rules.test.ts",
    "test:rules:with-emulator": "firebase emulators:start --only firestore \"jest tests/firebase/security-rules.test.ts\"",
    "test:task-rules": "jest tests/firebase/task-rules.test.ts",
    "test:task-rules:with-emulator": "firebase emulators:exec --only firestore 'jest tests/firebase/task-rules.test.ts --detectOpenHandles'",
    "test:all-rules": "jest 'tests/firebase/.*-rules.test.ts'",
    "test:all-rules:with-emulator": "firebase emulators:exec --only firestore 'jest \"tests/firebase/.*-rules.test.ts\" --detectOpenHandles'",
    "test:firebase": "firebase emulators:exec --only firestore \"jest tests/firebase/security-rules.test.ts tests/firebase/task-rules.test.ts\"",
    "test:with-emulators": "npm run firebase:emulators & sleep 5 && jest && kill $(lsof -t -i:8080)",
    "setup": "node scripts/setup.js"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "@mui/material-nextjs": "^6.4.3",
    "@prisma/client": "^6.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase-admin": "^13.2.0",
    "next": "^15.2.4",
    "next-pwa": "^5.6.0",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "uuid": "^11.1.0",
    "webpack": "^5.98.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@babel/plugin-syntax-flow": "^7.26.0",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/plugin-transform-typescript": "^7.27.0",
    "@babel/preset-typescript": "^7.27.0",
    "@eslint/eslintrc": "^3.3.1",
    "@firebase/rules-unit-testing": "^4.0.1",
    "@next/bundle-analyzer": "^15.2.4",
    "@next/eslint-plugin-next": "^15.2.4",
    "@playwright/test": "^1.51.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cookie": "^1.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19",
    "@types/serviceworker": "^0.0.127",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.8.4",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "dotenv": "^16.4.7",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.2.7",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-prettier": "^5.2.5",
    "firebase": "^11.5.0",
    "firebase-tools": "^13.5.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^12.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-mocks-http": "^1.16.2",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5",
    "typescript-eslint": "^8.28.0"
  },
  "description": "{{YOUR_PROJECT_DESCRIPTION}}",
  "repository": {
    "type": "git",
    "url": "{{YOUR_REPOSITORY_URL}}"
  },
  "overrides": {
    "whatwg-url": "^14.0.0",
    "tr46": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "last 2 versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
