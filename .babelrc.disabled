{
  "presets": [
    ["next/babel", {
      "preset-react": {
        "runtime": "automatic",
        "importSource": "react"
      }
    }],
    ["@babel/preset-typescript", { 
      "isTSX": true, 
      "allExtensions": true,
      "runtime": "automatic"
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-typescript", { 
      "allowDeclareFields": true,
      "runtime": "automatic"
    }]
  ]
} 